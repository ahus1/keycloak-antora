{
  "name": "keycloak-antora",
  "version": "1.0.0",
  "description": "Keycloak Antora Proof-of-Concept",
  "author": "Alexander Schwartz",
  "private": true,
  "scripts": {
    "author-without-pdf": "antora --stacktrace --redirect-facility static antora-playbook-author-without-pdf.yml",
    "author-with-pdf": "antora --stacktrace --redirect-facility static antora-playbook-author-with-pdf.yml",
    "build": "antora --stacktrace --redirect-facility static antora-playbook.yml",
    "serve": "http-server _site -c-1 -p 5000 -a 127.0.0.1"
  },
  "devDependencies": {
    "@antora/cli": "^3.1.6",
    "@antora/lunr-extension": "^1.0.0-alpha.8",
    "@antora/pdf-extension": "^1.0.0-alpha.7",
    "@antora/site-generator": "^3.1.5",
    "asciidoctor-highlight.js": "^0.4.0",
    "asciidoctor-kroki": "^0.18.1",
    "http-server": "^14.1.1"
  }
}
